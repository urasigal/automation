<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<!-- The goal of the suite is to verify an feeder server ability to establish SSH connection to a SSH server and make a reverse tunnel connection-->
<suite name="Feeder - SHH functionality suite" verbose="5" data-provider-thread-count="1">
	<test name="Feeder - upload a private key test">
		<parameter name="userName" value="admin" />
		<parameter name="userPass" value="1234" />
		<parameter name="login_ip" value="10.7.0.65" />
		<parameter name="uiport" value="4200" />
		<parameter name="testid" value="351" />    
		<classes>
		    <class name="com.zixi.testing.FeederPostKeyTest"/>
		</classes>
	</test>
	<test name="Feeder - set SSH server parameters">
		<parameter name="userName" value="admin" />
		<parameter name="userPass" value="1234" />
		<parameter name="login_ip" value="10.7.0.65" />
		<parameter name="uiport" value="4200" />
		<parameter name="host" value="10.7.0.66" />
		<parameter name="ssh_port" value="22" />
		<parameter name="ssh_user" value="root" />
		<parameter name="testid" value="351" />    
		<classes>
		    <class name="com.zixi.testing.FeederSettingsTest"/>
		</classes>
	</test>
	<test name="Feeder - set SSH reverse tunnel parameters">
		<parameter name="userName" value="admin" />
		<parameter name="userPass" value="1234" />
		<parameter name="login_ip" value="10.7.0.65" />
		<parameter name="uiport" value="4200" />
		<parameter name="on" value="1" />
		<parameter name="target_port" value="0" />
		<parameter name="target_ip" value="127.0.0.1" />
		<parameter name="remote_port" value="9933" />
		<parameter name="testid" value="351" />    
		<classes>
		    <class name="com.zixi.testing.FeederReverseTunnelParametersTest"/>
		</classes>
	</test>
	<test name="Feeder - test reverse tunnel remote socket">
		<parameter name="userName" value="root" />
		<parameter name="sshPass" value="zixiroot1234" />
		<parameter name="login_ip" value="10.7.0.66" />
		<parameter name="ssh_port" value="22" />
		<parameter name="reverse_port" value="9933" />
		<parameter name="ssh_user" value="root" />
		<parameter name="testid" value="351" />    
		<classes>
		    <class name="com.zixi.testing.FeederReverseTunnelTest"/>
		</classes>
	</test>                                              
</suite>