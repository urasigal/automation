<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Manual load test setup suite - creates a load setup" verbose="5" data-provider-thread-count="1">
<!-- This setup made up from 3 broadcaster servers: the first broadcaster server has a file input stream,
	then this file input is going as multiple output UDP streams to the second broadcaster server,
	then the accepted UDP from the second broadcaster are Zixi pushed to the third broadcaster
	server and in the end the multiple push inputs from the third broadcaster UDP outputed 
	to the first broadcaster so kind of circle is accepted -->
<test name="Broadcaster-Upload DTLS cetrificate to server">
		<parameter name="userName" value="admin" />
		<parameter name="userPass" value="1234" />
		<parameter name="login_ip" value="10.7.0.62" />
		<parameter name="uiport" value="4444" />
		<parameter name="testid" value="7861" />    
		<classes>
		    <class name="com.zixi.testing.FeederPostKeyTest">
			    <methods>
		        	<include name="uploadSslCertificateToBroadcasterServer" />
		     	</methods>
		  	</class>
		</classes>
	</test>
	<test name="Broadcaster-Upload DTLS private key to server">
